<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<navigation-rule>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/accueil.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	

	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>com.jmd.test.jsf.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>nom</property-name>
			<value>your name</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>langueApp</managed-bean-name>
		<managed-bean-class>com.lang.util.LangueApp</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>		
	</managed-bean>
	<managed-bean>
		<description>Profile manager</description>
		<managed-bean-name>profileBean</managed-bean-name>
		<managed-bean-class>com.services.ProfileBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>uploadFileBean</managed-bean-name>
		<managed-bean-class>com.services.UploadFileBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	

	<application>
		<locale-config>
			<default-locale>fr</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>


</faces-config>
